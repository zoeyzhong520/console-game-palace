@charset "utf-8";
.usercenter-leaderboards-container {
	display: flex;
	position: relative;
	padding-bottom: 30rpx;
}
.usercenter-leaderboards-box {
	display: flex;
	flex-wrap: wrap;
	position: relative;
}
.usercenter-leaderboards-box-item {
	margin-left: 30rpx;
	margin-bottom: 30rpx;
	width: 330rpx;
	height: 140rpx;
	background: #F5F6F7;
	border-radius: 16rpx;
	
	display: flex;
	align-items: center;
	justify-content: center;
	position: relative;
	
	font-size: 50rpx;
	line-height: 70rpx;
	color: #FFFFFF;
	font-weight: bold;
	text-align: center;
	text-shadow: #909399 3rpx 3rpx 3rpx;
}

